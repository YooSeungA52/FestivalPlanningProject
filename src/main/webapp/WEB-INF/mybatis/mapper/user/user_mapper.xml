<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="user_mapper">

	<insert id="insert_user" parameterType="UserDto">
		insert into f_user
		values ( #{login_Id}, #{login_Pw}, #{name}, #{email}, #{phone_Number} )
	</insert>

	<select id="select_user" resultType="UserDto"
		parameterType="UserDto">
		select login_Id, login_Pw, name, email, phone_Number
		from f_user
	</select>

	<select id="select_user2" resultType="UserDto"
		parameterType="UserDto">
		select login_Id, login_Pw, name, email, phone_Number
		from f_user
	</select>

</mapper>
